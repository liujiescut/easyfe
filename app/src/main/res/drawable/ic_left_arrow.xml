<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotY="50%" android:pivotX="10%">
            <shape android:shape="rectangle">
                <size android:width="20dp" android:height="4dp"></size>
                <solid android:color="@android:color/white"></solid>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="-45" android:toDegrees="-45" android:pivotY="50%" android:pivotX="10%">
            <shape android:shape="rectangle">
                <size android:width="20dp" android:height="4dp"></size>
                <solid android:color="@android:color/white"></solid>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:pivotX="50%" android:pivotY="50%" android:fromDegrees="0" android:toDegrees="0">
            <shape android:shape="rectangle">
                <size android:width="20dp" android:height="2dp"/>
                <solid android:color="@android:color/white"/>
                <corners android:radius="1dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>